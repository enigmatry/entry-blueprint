{
  "name": "enigmatry-entry-blueprint-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "jest --coverage=false",
    "test:ci": "jest --ci --coverage=true",
    "lint": "npm run lint:ts && npm run lint:styles",
    "lint:ts": "npx ng lint --fix",
    "lint:styles": "stylelint --fix src/**/*.scss",
    "audit:ci": "npm audit --json|npm-audit-reporter-teamcity",
    "nswag": "node_modules\\.bin\\nswag run nswag.json",
    "codegen:run": "entry-codegen --source-assembly ../Enigmatry.Entry.Blueprint.CodeGeneration.Setup/bin/Debug/net8.0/Enigmatry.Entry.Blueprint.CodeGeneration.Setup.dll --destination-directory ./src/app/features",
    "codegen:run-i18n": "entry-codegen --source-assembly ../Enigmatry.Entry.Blueprint.CodeGeneration.Setup/bin/Debug/net8.0/Enigmatry.Entry.Blueprint.CodeGeneration.Setup.dll --destination-directory ./src/app/features --enable-i18n true",
    "codegen:install": "dotnet tool install enigmatry.entry.codegeneration.tools -g",
    "codegen:update": "dotnet tool update enigmatry.entry.codegeneration.tools -g",
    "i18n": "ng extract-i18n --format json --output-path src/i18n --out-file messages.en.json & sortjson src/i18n"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.3.9",
    "@angular/cdk": "^17.3.9",
    "@angular/common": "^17.3.9",
    "@angular/compiler": "^17.3.9",
    "@angular/core": "^17.3.9",
    "@angular/forms": "^17.3.9",
    "@angular/material": "^17.3.9",
    "@angular/material-date-fns-adapter": "^17.3.9",
    "@angular/platform-browser": "^17.3.9",
    "@angular/platform-browser-dynamic": "^17.3.9",
    "@angular/router": "^17.3.9",
    "@azure/msal-browser": "3.2.0",
    "@enigmatry/entry-components": "17.0.0",
    "@enigmatry/entry-form": "17.0.0",
    "@microsoft/applicationinsights-web": "3.1.0",
    "@ngx-formly/core": "6.3.1",
    "@ngx-formly/material": "6.3.1",
    "date-fns": "^2.30.0",
    "rxjs": "^7.8.1",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.6"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.7",
    "@angular/cli": "^17.3.7",
    "@angular/compiler-cli": "^17.3.9",
    "@angular/localize": "^17.3.9",
    "@enigmatry/eslint-config": "17.0.0",
    "@enigmatry/scss-foundation": "17.0.0",
    "@enigmatry/stylelint-config": "17.0.0",
    "@types/jest": "29.5.12",
    "jest": "29.7.0",
    "jest-junit": "^16.0.0",
    "jest-preset-angular": "14.0.4",
    "json-sort-cli": "^4.0.3",
    "npm-audit-reporter-teamcity": "0.4.0",
    "nswag": "^14.0.7",
    "typescript": "5.4.5"
  }
}