{
  "DbContext": {
    "SensitiveDataLoggingEnabled": false,
    "ConnectionResiliencyMaxRetryCount": 10,
    "ConnectionResiliencyMaxRetryDelay": "0.00:00:30",
    "UseAccessToken": true
  },
  "ConnectionStrings": {
    "AppDbContext": "__sqlDbConnectionString__"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "__defaultLogLevel__",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.EntityFrameworkCore": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Logger",
        "Args": {
          "configureLogger": {
            "WriteTo": [
              {
                "Name": "File",
                "Args": {
                  "path": "__rollingLogFilePath__",
                  "rollingInterval": "Day",
                  "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{AppVersion}] [{User}-{RequestId}-{Address}] [{Tenant}] [{Level}] [thread:{ThreadId}] [{SourceContext}] {Message}{NewLine}{Exception}"
                }
              }
            ],
            "Filter": [
              {
                "Name": "ByExcluding",
                "Args": {
                  "expression": "Contains(EventId.Name, 'SecurityLog')"
                }
              }
            ]
          }
        }
      },
      {
        "Name": "Logger",
        "Args": {
          "configureLogger": {
            "WriteTo": [
              {
                "Name": "File",
                "Args": {
                  "path": "__rollingLogSecurityFilePath__",
                  "rollingInterval": "Day",
                  "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{AppVersion}] [{User}-{RequestId}-{Address}] [{Tenant}] [{Level}] [thread:{ThreadId}] [{SourceContext}] {Message}{NewLine}{Exception}"
                }
              }
            ],
            "Filter": [
              {
                "Name": "ByIncludingOnly",
                "Args": {
                  "expression": "Contains(EventId.Name, 'SecurityLog')"
                }
              }
            ]
          }
        }
      }
    ]
  },
  "Scheduling": {
    "Host": {
      "quartz.scheduler.instanceName": "Enigmatry.Entry.Blueprint.Scheduler"
    },
    "Jobs": {
      "CleanOldProductsJob": {
        "RunOnStartup": true,
        "Enabled": true,
        "Cronex": "__cleanOldProductsJobCronExpression__",
        "Args": {
          "DeactivateOlderThan": "__cleanOldProductsJobDeactivateOlderThan__"
        }
      }
    }
  }
}