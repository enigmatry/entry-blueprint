	<s:String x:Key="/Default/PatternsAndTemplates/LiveTemplates/Template/=01413C014EE2184E93CB0CDECC2E24B8/Text/@EntryValue">using AutoMapper;&#xD;
using AutoMapper.QueryableExtensions;&#xD;
using $ROOTNAMESPACE$.Domain.$NAME$s;&#xD;
using Enigmatry.Entry.Core.Data;&#xD;
using Enigmatry.Entry.Core.EntityFramework;&#xD;
using Enigmatry.Entry.Core.Paging;&#xD;
using JetBrains.Annotations;&#xD;
&#xD;
namespace $NAMESPACE$;&#xD;
&#xD;
public static class $CLASS$&#xD;
{&#xD;
    [PublicAPI]&#xD;
    public class Request : PagedRequest&lt;Response.Item&gt;&#xD;
    {&#xD;
        public string Keyword { get; set; } = String.Empty;&#xD;
    }&#xD;
&#xD;
    [PublicAPI]&#xD;
    public static class Response&#xD;
    {&#xD;
        [PublicAPI]&#xD;
        public class Item&#xD;
        {&#xD;
            public Guid Id { get; set; }&#xD;
            public string Name { get; set; } = String.Empty;&#xD;
            public DateTimeOffset CreatedOn { get; set; }&#xD;
            public DateTimeOffset UpdatedOn { get; set; }&#xD;
        }&#xD;
&#xD;
        [UsedImplicitly]&#xD;
        public class MappingProfile : Profile&#xD;
        {&#xD;
            public MappingProfile() =&gt; CreateMap&lt;$NAME$, Item&gt;();&#xD;
        }&#xD;
    }&#xD;
&#xD;
	[UsedImplicitly]&#xD;
    public class RequestHandler : IPagedRequestHandler&lt;Request, Response.Item&gt;&#xD;
    {&#xD;
        private readonly IRepository&lt;$NAME$&gt; _repository;&#xD;
        private readonly IMapper _mapper;&#xD;
&#xD;
        public RequestHandler(IRepository&lt;$NAME$&gt; repository, IMapper mapper)&#xD;
        {&#xD;
            _repository = repository;&#xD;
            _mapper = mapper;&#xD;
        }&#xD;
&#xD;
        public async Task&lt;PagedResponse&lt;Response.Item&gt;&gt; Handle(Request request, CancellationToken cancellationToken) =&gt;&#xD;
            await _repository.QueryAll()&#xD;
                .QueryByName(request.Keyword)&#xD;
                .ProjectTo&lt;Response.Item&gt;(_mapper.ConfigurationProvider, cancellationToken)&#xD;
                .ToPagedResponseAsync(request, cancellationToken);&#xD;
    }&#xD;
}&#xD;
&#xD;
