{
    "UseDeveloperExceptionPage": false,
    "DbContext": {
        "SensitiveDataLoggingEnabled": false,
        "ConnectionResiliencyMaxRetryCount": 10,
        "ConnectionResiliencyMaxRetryDelay": "0.00:00:30",
        "UseAccessToken": true
    },
    "ConnectionStrings": {
        "AppDbContext": "__sqlDbConnectionString__"
    },
    "App": {
        "SampleKeyVaultSecret": "keyVaultSecret",
        "Smtp": {
            "Server": "smtp.sendgrid.net",
            "Port": "465",
            "Username": "keyVaultSecret",
            "Password": "keyVaultSecret",
            "From": "blueprint@enigmatry.com",
            "UsePickupDirectory": false,
            "PickupDirectoryLocation": ""
        },
        "AzureAd": {
            "Instance": "__azureAdInstance__",
            "ClientId": "__azureAdClientId__",
            "Domain": "__azureAdDomain__",
            "SignUpSignInPolicyId": "__azureAdSignUpSignInPolicyId__"
        }
    },
  "Serilog": {
    "MinimumLevel": {
      "Default": "__defaultLogLevel__",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.EntityFrameworkCore": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Logger",
        "Args": {
          "configureLogger": {
            "WriteTo": [
              {
                "Name": "File",
                "Args": {
                  "path": "__rollingLogFilePath__",
                  "rollingInterval": "Day",
                  "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{AppVersion}] [{RequestId}] [{Tenant}] [{Level}] [thread:{ThreadId}] [{SourceContext}] {Message}{NewLine}{Exception}"
                }
              }
            ],
            "Filter": [
              {
                "Name": "ByExcluding",
                "Args": {
                  "expression": "Contains(EventId.Name, 'SecurityLog')"
                }
              }
            ]
          }
        }
      },
      {
        "Name": "Logger",
        "Args": {
          "configureLogger": {
            "WriteTo": [
              {
                "Name": "File",
                "Args": {
                  "path": "__rollingLogSecurityFilePath__",
                  "rollingInterval": "Day",
                  "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{AppVersion}] [{User}-{RequestId}-{Address}] [{Tenant}] [{Level}] [thread:{ThreadId}] [{SourceContext}] {Message}{NewLine}{Exception}"
                }
              }
            ],
            "Filter": [
              {
                "Name": "ByIncludingOnly",
                "Args": {
                  "expression": "Contains(EventId.Name, 'SecurityLog')"
                }
              }
            ]
          }
        }
      }
    ]
  },
    "HealthChecks": {
        "MaximumAllowedMemoryInMegaBytes": "200",
        "TokenAuthorizationEnabled": true,
        "RequiredToken": "__healthChecksToken__"
    },
    "KeyVault": {
        "Enabled": false,
        "Name": "__keyVaultName__"
    }
}