{
  "name": "enigmatry-blueprint-app-tests",
  "description": "blueprint e2e tests",
  "main": "index.js",
  "scripts": {
    "pretest": "rimraf screenshots && rimraf reports",
    "test": "testcafe chrome tests/**/*.fixture.ts --env=local --user=user1@example.com  --selector-timeout 10000 --debug-on-fail -S -s screenshots",
    "test:json": "npm test -- --reporter cucumber-json:reports/report.json --reporter-app-name='My App' --reporter-app-version='x.y.z'",
    "test:live": "testcafe chrome tests/**/*.live.ts --live --env=local --user=user1@example.com  --selector-timeout 10000",
    "test:teamcity": "testcafe 'chrome --ignore-certificate-errors' tests/**/*.fixture.ts --env=devci --user=user1@example.com  --selector-timeout 10000 --reporter teamcity -S -s screenshots --quarantine-mode",
    "tsc": "tsc",
    "tsc:init": "tsc --init",
    "tslint:init": "tslint --init",
    "lint": "tslint --format stylish --config tslint.json -e **/*.d.ts **/*.ts",
    "prebuild": "rimraf build",
    "report": "ts-node report-generator.ts"
  },
  "dependencies": {
    "@types/minimist": "1.2.0",
    "@types/node": "12.6.8",
    "chalk": "2.4.2",
    "cross-env": "5.2.0",
    "jsome": "2.5.0",
    "minimist": "1.2.0",
    "rimraf": "2.6.3",
    "slash": "2.0.0",
    "testcafe": "1.2.1",
    "testcafe-reporter-cucumber-json": "1.2.0",
    "testcafe-reporter-teamcity": "1.0.10",
    "ts-node": "8.2.0",
    "typescript": "3.5.1"
  },
  "devDependencies": {
    "eslint": "5.16.0",
    "multiple-cucumber-html-reporter": "1.11.8",
    "prettier": "1.16.4",
    "tslint": "5.17.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-config-standard": "8.0.1"
  }
}